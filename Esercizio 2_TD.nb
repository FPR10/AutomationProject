         FractionBox["25", "9"], 
         SuperscriptBox[
          RowBox[{"(", 
            FractionBox["2", "5"], ")"}], 
          StyleBox["k", "TI"]], "1", 
         RowBox[{"(", 
           StyleBox["k", "TI"], ")"}], "-", 
         FractionBox["35", "9"], 
         StyleBox["k", "TI"], 
         SuperscriptBox[
          RowBox[{"(", 
            FractionBox["2", "5"], ")"}], 
          RowBox[{
            StyleBox["k", "TI"], "-", "1"}]], "1", 
